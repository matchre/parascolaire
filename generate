#!/bin/bash
# ./generate_txt.py

./generate_markdown calculette

python3 generate_html_from_py.py \
    progra_equivalences_multilang.py \
    --template=pytext_template.html \
    --out=progra_equivalences_{lang}.html \
    --out-raw=progra_equivalences_{lang}.py

for n in theorie1_egal_if; do
python3 ./generate_html_from_py.py \
    ${n}_multilang.py \
    --template pytext_template.html \
    --out ${n}_{lang}.html \
    --out-raw ${n}_{lang}.py
done

for n in theorie1_egal_if theorie2_liste_while theorie3_fonctions_et_objets; do
python3 ./generate_html_from_py.py \
    ${n}.py \
    --template pytext_template.html \
    --out ${n}.html \
    --out-raw ${n}.py
done

python3 generate_multilang.py --langs fr en
python3 generate_html.py
# ./generate_ln.py
python3 generate_ls.py # keep last
cd home/2017-2018
python3 generate_index.py
cd ../..

